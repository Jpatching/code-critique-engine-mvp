{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flask: Debug Server",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "server.py",
                "FLASK_DEBUG": "1",
                "GEMINI_API_KEY": "AIzaSyBf0cGONzMrHzsGYY9Cm19g--B5PYrtWn8",
                "POCKETBASE_URL": "http://127.0.0.1:8090"
            },
            "args": [
                "run",
                "--host=127.0.0.1",
                "--port=5000",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true,
            "justMyCode": true,
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "GEMINI_API_KEY": "AIzaSyBf0cGONzMrHzsGYY9Cm19g--B5PYrtWn8",
                "POCKETBASE_URL": "http://127.0.0.1:8090"
            }
        }
    ]
}